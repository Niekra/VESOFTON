/*!
 * \brief teken een lijn.
 * \param paramter int.
 */
int LogicLayer::setCommand(char *buf){
	int i = 1;
	char str[100];
	strcpy(str, buf);
	char *saveptr;
	char *out;

	out = strtok_r(str, " ", &saveptr);

	switch(out){
	case 'repeat':
		break;
	case 'buffer':
		break;
	case 'exec':
		break;
	case 'clear':
		break;
	case 'wait':
		break;
	default :
		i = 0;
		break;
	}

	foo = strtok_r(NULL, " ", &saveptr);
}

