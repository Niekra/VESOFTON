/*!
 * \brief teken een lijn.
 * \param paramter int.
 */
int LogicLayer::setCommand(char *buf){
	int i = 1;
	char str[100];
	strcpy(str, buf);
	char *saveptr;
	char *out;
	char *outArray[10];

	out = strtok_r(str, " ", &saveptr);
	outArray[0] = out;

	while(out != NULL){
		out = strtok_r(NULL, " ", &saveptr);
		outArray[i] = out;
		i++;
	}

	//out = strtok_r(str, " ", &saveptr);
/*
	if(out == "repeat"){

	}else if(out=="buffer"){

	}else if(out=="exec"){

	}else if(out=="clear"){

	}else if(out=="wait"){

	}else{
		command.type = out;
		i = 1;

	}*/


}

